    <section class="informacion">
        <h3>Informacion Producto Nuevo</h3>
        <div class="box"><input type="text" name="text" class="input" placeholder="Marca"></div>
        <div class="box"><input type="text" name="text" class="input" placeholder="Modelo"></div>
        <div class="box"><input type="text" name="text" class="input" placeholder="Año"></div>
        <div class="box"><input type="text" name="text" class="input" placeholder="Kilometraje"></div>
        <div class="box"><input type="text" name="text" class="input" placeholder="Potencia (hp)"></div>
        <div class="box-label">
        <label for=""> Fecha de publicacion</label>
        <input type="date" name="text" class="input" placeholder="Fecha de publicacion"></div>
        <div class="box"><input type="text" name="text" class="input" placeholder="Precio"></div>
        <div class="box"><input type="text" name="text" class="input" placeholder="Color"></div>

        <label class="container"> Destacado
            <input type="checkbox" checked="checked">
            <div class="checkmark"></div>
          </label>

          
        <label class="container"> Banner
            <input type="checkbox" checked="checked">
            <div class="checkmark"></div>
          </label>

          
        <label class="container"> Promoción
